{
  "name": "@eidellev/adonis-stardust",
  "version": "1.1.0",
  "private": false,
  "description": "Use adonis routes in the client",
  "repository": "https://github.com/eidellev/adonis-stardust",
  "bugs": "https://github.com/eidellev/adonis-stardust/issues",
  "main": "build/providers/StardustProvider.js",
  "browser": "build/client/stardust.js",
  "files": [
    "build/providers",
    "build/client"
  ],
  "adonisjs": {
    "providers": [
      "@eidellev/adonis-stardust"
    ]
  },
  "license": "MIT",
  "scripts": {
    "lint": "tsc --noEmit && eslint . --ext=ts",
    "lint:fix": "eslint . --ext=ts --fix",
    "clean": "rimraf build",
    "build": "cross-env npm run clean && npm run build:node && npm run build:client",
    "build:node": "tsc",
    "build:client": "esbuild ./build/client/stardust.js --outfile=build/client/stardust.js --allow-overwrite --bundle --platform=node --target=chrome91,edge90,firefox90,safari13",
    "watch": "cross-env npm run clean && tsc -w",
    "test": "nyc node japaFile.js"
  },
  "peerDependencies": {
    "@adonisjs/core": ">=5"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.3.2",
    "@adonisjs/mrm-preset": "^4.1.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@adonisjs/view": "^6.1.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "adonis-preset-ts": "^2.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.25",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "japa": "^3.1.1",
    "lint-staged": "^11.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.7",
    "typescript": "^4.4.2"
  }
}
