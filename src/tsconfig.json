{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "outDir": "../client"
  },
  "include": ["./client"]
}
